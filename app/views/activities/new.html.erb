<h1>New Activity</h1>

<%= render 'form', activity: @activity %>

<div class="row justify-content-center">
    <div class="col m-3">
        <!-- CSV Uploader -->
        <h3> Import activities </h3>
        <%= form_tag import_activities_path, multipart: true do %>
        <%= file_field_tag :file %>
        <%= submit_tag "Upload activities" %>
        <%end %>
    </div>
</div>